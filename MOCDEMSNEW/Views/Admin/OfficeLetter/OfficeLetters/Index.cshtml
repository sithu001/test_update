    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_AdminLayout";
}
<div class="container custom-bg mt-5">
    <div class="mb-5">
        <a href="javascript:history.back()" class="text-secondary text-decoration-none fw-bold">
            <i class="fa-solid fa-arrow-left me-1"></i> Go Back
        </a>
    </div>
    <div class="text-center mb-5">
        <h5 class="text-dark text-uppercase fw-bold">
            <i class="fa-solid fa-envelope fs-5 px-2"></i>Office Letter
        </h5>
    </div>

    <div class="table-responsive">
        <table class="table custom-table">
            <thead>
                <tr>
                    <th>
                        No.
                    </th>
                    <th>
                        Subject
                    </th>
                    <th>
                        Date
                    </th>
                    <th style="width: 150px;">
                        Action
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td data-label="No.">1</td>
                    <td data-label="Subject">
                        “Fully Online Licensing System တွင် Google Authenticator
                        ပြောင်းလဲအသုံးပြုမှု နှင့် Online CO စနစ်အသုံးပြုမှုနှင့်ပတ်သက်သည့် အသိပညာပေးဆွေးနွေးပွဲ”
                        ကျင်းပခဲ့မှုအပေါ် အစီရင်ခံစာတင်ပြခြင်းကိစ္စ
                    </td>
                    <td data-label="Date">2020-Apr-29</td>
                    <td data-label="Action">
                        <div class="action">
                            <a href="#" class="btn btn-red rounded-pill text-dark fw-bold"
                               onclick="return confirm('Are you sure to delete')">
                                <i class="fa-solid fa-trash fs-5"></i>Delete
                            </a>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td data-label="No.">2</td>
                    <td data-label="Subject">စီးပွားရေးနှင့်ကူးသန်းရောင်းဝယ်ရေးဝန်ကြီးဌာန၊ ရန်ကုန်တိုင်းဒေသကြီးရုံးခွဲ၌ မြန်မာနိုင်ငံသားများဘဏ်လီမိတက်၏ ဘဏ်ခွဲငယ်(Mini Branch) ကို ငှားရမ်းဖွင့်လှစ်ခွင့်ပြုပါရန် တင်ပြခြင်းကိစ္စ</td>
                    <td data-label="Date">2020-Apr-29</td>
                    <td data-label="Action">
                        <div class="action">
                            <a href="#" class="btn btn-red rounded-pill text-dark fw-bold"
                               onclick="return confirm('Are you sure to delete')">
                                <i class="fa-solid fa-trash fs-5"></i>Delete
                            </a>
                        </div>
                    </td>
                </tr>

            </tbody>
        </table>
    </div>
</div>
<div>
    @await Html.PartialAsync("_AdminFooter")
</div>